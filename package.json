{
  "name": "gesties",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:db": "./src/scripts/start-db.sh",
    "rayuela": "node dist/scripts/rayuela.js",
    "rayuela:dev": "ts-node -r tsconfig-paths/register src/scripts/rayuela.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "pretypeorm": "(rm ormconfig.json || :) && ts-node -r tsconfig-paths/register src/scripts/write-type-orm-config.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -n",
    "typeorm:migration:run": "npm run typeorm -- migration:run",
    "typeorm:migration:revert": "npm run typeorm -- migration:revert",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.5.1",
    "@nestjs/typeorm": "^7.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "clear": "^0.1.0",
    "cli-progress": "^3.7.0",
    "commander": "^5.1.0",
    "dotenv": "^8.2.0",
    "helmet": "^3.22.0",
    "jimp": "^0.10.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-geocoder": "^3.27.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-ldapauth": "^2.1.4",
    "pg": "^8.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "temp-dir": "^2.0.0",
    "typeorm": "^0.2.24",
    "unzipper": "^0.10.10",
    "xlsx": "^0.16.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/cli-progress": "^3.7.0",
    "@types/express": "^4.17.3",
    "@types/hapi__joi": "^17.1.3",
    "@types/jest": "25.1.4",
    "@types/jimp": "^0.2.28",
    "@types/node": "^13.9.1",
    "@types/node-geocoder": "^3.24.1",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/pg": "^7.14.3",
    "@types/rimraf": "^3.0.0",
    "@types/supertest": "^2.0.8",
    "@types/unzipper": "^0.10.3",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
